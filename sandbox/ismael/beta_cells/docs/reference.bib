issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Animal,Animals,Cell Movement,Cells,Cerebral Cortex,Contrast Media,Corpus Callosum,Cultured,Cultured: transplantation,Electron Spin Resonance Spectroscopy,Fetal Tissue Transplantation,Genes,Green Fluorescent Proteins,Heterologous,Hippocampus,Hippocampus: cytology,Imaging,Infarction,Luminescent Proteins,Luminescent Proteins: analysis,Luminescent Proteins: genetics,Magnetic Resonance Imaging,Magnetic Resonance Imaging: methods,Male,Mice,Middle Cerebral Artery,Middle Cerebral Artery: pathology,Middle Cerebral Artery: therapy,Models,Neurons,Neurons: transplantation,Rats,Reporter,Sensitivity and Specificity,Stem Cell Transplantation,Three-Dimensional,Transgenic,Transplantation,Wistar},

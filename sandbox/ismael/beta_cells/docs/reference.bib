institution = {Department of Physics and Astronomy and Center for Theoretical Physics, Seoul National University, Seoul, Korea.},
issn = {0006-3495},
journal = {Biophysical journal},
keywords = {Aging,Aging: physiology,Animals,Biological,Cell Enlargement,Cell Proliferation,Cells,Computer Simulation,Cultured,Inbred BALB C,Islets of Langerhans,Islets of Langerhans: cytology,Islets of Langerhans: physiology,Male,Mice,Models},
